# WellSync AI - Environment Configuration
# Rename this file to .env and add your API keys within the quotes but without spaces (e.g. GEMINI_API_KEY=AIz...)

# Flask Settings
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_SECRET_KEY=dev-secret-key-change-in-production
FLASK_PORT=5000
DEBUG_MODE=True

# LLM Configuration
# Options: gemini, openai, groq
# Default is gemini as it is often free/accessible
LLM_PROVIDER=gemini
LLM_MODEL=gemini/gemini-3-flash-preview

# API Keys (REQUIRED: Replace with your actual keys)
GEMINI_API_KEY=
OPENAI_API_KEY=
GROQ_API_KEY=

# Agent Settings
AGENT_TEMPERATURE=0.7
AGENT_MAX_TOKENS=2000
MAX_CONCURRENT_AGENTS=4

# Database settings
DATABASE_URL=sqlite:///data/databases/wellsync.db
# REDIS_URL=redis://localhost:6379/0 # Optional

# Safety Limits
MAX_WORKOUT_INTENSITY=0.9
MIN_SLEEP_HOURS=6